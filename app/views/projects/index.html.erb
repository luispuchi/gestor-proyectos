<h1>Ingresa un Proyecto</h1><hr><hr>

<%= form_for :project, url: projects_new_path do |f| %>
 
  <%= f.label(:name,'nombre')%>
	<%= f.text_field :name %>
  <%= f.label(:description, 'Descripción')%>
	<%= f.text_field :description %>
  <%= f.label(:start_date, 'Fecha Inicio')%>
  <%= f.date_field :start_date %>
  <%= f.label(:end_date,'Fecha Término') %>
  <%= f.date_field :end_date %>
  <%= f.label(:content,'Estado')%>
  <%= select(:project, :content,[['Propuesta'], ['En progreso'], ['Terminado']], include_blank: false)%>
	<%= f.submit 'Guardar' %>
<% end %>
